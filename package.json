{
  "version": "1.0.0",
  "name": "crm-powerbi-viewer",
  "displayName": "Power BI Viewer (v1.0)",
  "description": "Embed tiles and reports from Power BI into Dynamics 365 Sales and Service Forms and Dashboards.",
  "homepage": "http://crm-powerbi-viewer.heiigjen.com/",
  "license": "MIT",
  "author": "Trond Aarskog (trond<alphahere>heiigjen.com) from Heiigjen Solutions",
  "bugs": {
    "url": "https://github.com/taarskog/crm-powerbi-viewer/issues"
  },
  "private": true,
  "keywords": [
    "xrm",
    "crm",
    "d365",
    "dynamics 365",
    "powerbi"
  ],
  "scripts": {
    "watch": "gulp watch --env debug",
    "build": "gulp build --env production && powershell ./tools/packSolution.ps1",
    "build-debug": "gulp build --env debug && powershell ./tools/packSolution.ps1",
    "deploy": "npm run build && powershell ./tools/deployFiles.ps1",
    "deploy-debug": "npm run build-debug && powershell ./tools/deployFiles.ps1",
    "clean": "gulp clean"
  },
  "devDependencies": {
    "gulp": "3.9.1",
    "gulp-plumber": "1.2.0",
    "gulp-uglify": "3.0.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-if": "2.0.2",
    "gulp-sass": "3.1.0",
    "gulp-css-svg": "1.2.3",
    "gulp-clean-css": "3.9.2",
    "gulp-htmlmin": "4.0.0",
    "gulp-header": "2.0.1",
    "gulp-newer": "1.4.0",
    "gulp-html-replace": "1.6.2",
    "gulp-preprocess": "2.0.0",
    "gulp-tslint": "8.1.3",
    "fancy-log": "1.3.2",
    "ansi-colors": "1.0.1",
    "tslint": "5.9.1",
    "minimist": "1.2.0",
    "del": "3.0.0",
    "typescript": "2.7.2",
    "browser-sync": "2.23.6",
    "bs-html-injector": "3.0.3",
    "browserify": "16.1.0",
    "tsify": "3.0.4",
    "vinyl-source-stream": "2.0.0",
    "vinyl-buffer": "1.0.1",
    "event-stream": "3.3.4",
    "babelify": "8.0.0",
    "babel-core": "6.26.0",
    "babel-preset-env": "1.6.1",
    "@types/browser-sync": "latest",
    "@types/gulp": "latest",
    "@types/browserify": "latest",
    "@types/adal-angular": "latest",
    "@types/handlebars": "latest",
    "@types/xrm": "latest"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "adal-angular": "1.0.15",
    "powerbi-client": "2.4.7",
    "handlebars": "4.0.11",
    "clipboard": "1.7.1"
  }
}
